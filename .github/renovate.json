{
  "extends": [
    "config:base",
    ":disableDependencyDashboard"
  ],
  "automerge": true,
  "ignoreDeps": [
    "org.spigotmc:spigot-api"
  ],
  "labels": [
    "renovate"
  ],
  "commitMessagePrefix": "chore: ",
  "rebaseWhen": "conflicted",
  "packageRules": [
    {
      "matchPackagePrefixes": ["software.amazon.awssdk"],
      "schedule": ["before 4am on the first day of the month"]
    }
  ]
}
